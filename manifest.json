{
  "manifest_version": 2,
  "name": "Your Extension Name",
  "version": "1.0",
  "description": "Description of your extension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
